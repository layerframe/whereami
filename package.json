{
  "name": "@layerframers/whereami",
  "version": "1.2.5",
  "description": "Tools that help with fetching location based info when building production SSR apps, e.g. NextJS. Client vs. Server, Staging vs. Production, etc.",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "auth": "npm adduser --scope=@layerframers",
    "deploy": "npm publish --access public --scope=@layerframers",
    "build": "node_modules/.bin/babel ./src/index.js -o ./lib/index.js",
    "test": "NODE_ENV=development mocha --require @babel/register test/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/layerframe/whereami.git"
  },
  "keywords": [
    "tool",
    "ssr",
    "nextjs",
    "whereami",
    "service",
    "node",
    "client",
    "server"
  ],
  "author": "Rob Sawyer (rob@layerframe.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/layerframe/whereami/issues"
  },
  "homepage": "https://github.com/layerframe/whereami#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "mocha": "^7.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2"
  }
}
